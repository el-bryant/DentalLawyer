<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Nomenclatura" >

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="?attr/actionBarSize"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Resina simple"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt02"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Resina compuesta"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt03"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Ionomero de vidrio"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt04"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Carilla directa"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt05"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Carilla indirecta"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt06"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Incrustaciones"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt07"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Profilaxis/alizaje"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt08"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Destartraje"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt09"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Blanqueamiento férulas"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Barniz fluorado (4 citas)"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Sellantes"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
