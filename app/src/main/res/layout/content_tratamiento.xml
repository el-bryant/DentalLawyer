                <Switch
                    android:id="@+id/swt12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="RX periapical"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Exodoncia simple"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Cirugía de 3ra. molar"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Cita control ortodoncia"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Endodoncia anterior"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt17"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Endodoncia de 1ra. premolar"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Endodoncia premolar"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Endodoncia posterior"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Pulpotomía"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt21"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Gingivectomía"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt22"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Espigo muñón"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt23"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Corona Veneer"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt24"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Corona metal porcelana"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt25"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Corona de porcelana libre de metal"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt26"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Prótesis removible metálica"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt27"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Prótesis removible acrílica"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt28"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Prótesis total"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Switch
                    android:id="@+id/swt29"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Ortodoncia"/>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</TableLayout>